{
  "manifest_version": 3,
  "name": "Mini Reloader",
  "version": "11.0",
  "description": "Automatically reloads the current tab at a specified interval.",
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "assets/icons/icon16.png",
      "48": "assets/icons/icon48.png",
      "128": "assets/icons/icon128.png"
    }
  },
  
  "background": {
    "service_worker": "assets/js/background.js"
  },
  "permissions": [
    "storage",
    "tabs"
  ],
  "icons": {
    "16": "assets/icons/icon16.png",
    "48": "assets/icons/icon48.png",
    "128": "assets/icons/icon128.png"
    
  }
}
